{
  "name": "vscode-messages",
  "displayName": "vscode-messages",
  "description": "Send messages to your co-workers",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.64.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:vscode-messages.sendCodeMessage",
    "onCommand:vscode-messages.sendRegularMessage",
    "onCommand:vscode-messages.webexLogin"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-messages.sendCodeMessage",
        "title": "Code message"
      },
      {
        "command": "vscode-messages.sendRegularMessage",
        "title": "Regular message"
      },
      {
        "command": "vscode-messages.webexLogin",
        "title": "Login with Cisco Webex"
      }
    ],
    "keybindings": [
      {
        "command": "vscode-messages.sendCodeMessage",
        "key": "ctrl+m",
        "mac": "cmd+m",
        "when": "editorHasSelection"
      },
      {
        "command": "vscode-messages.sendRegularMessage",
        "key": "ctrl+m",
        "mac": "cmd+m",
        "when": "!editorHasSelection"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/request": "^2.48.8",
    "@types/vscode": "^1.64.0",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "@vscode/test-electron": "^1.6.2",
    "eslint": "^8.1.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.3",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "express": "^4.17.2",
    "request": "^2.88.2"
  }
}
